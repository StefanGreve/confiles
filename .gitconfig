[user]
	email = greve.stefan@outlook.jp
	name = StefanGreve
[core]
	editor = nvim
	autocrlf = true
	sshCommand = C:/Windows/System32/OpenSSH/ssh.exe
    pager = delta
[interactive]
    diffFilter = delta --color-only
[delta]
    navigate = true
    light = false
[diff]
    colorMoved = default
[merge]
    conflictstyle = diff3
[init]
	defaultBranch = master
[alias]
	unstage = reset HEAD --
	uncommit = reset --soft HEAD^1
	last = log -1 HEAD
	graph = log --graph --oneline --decorate --abbrev-commit
[http]
	sslVerify = false
[credential "https://git.swisslab.de"]
	provider = bitbucket
[safe]
	directory = *
[help]
	autocorrect = 30
